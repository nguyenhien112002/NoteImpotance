-- search table in schemas
SELECT TABLE_NAME , TABLE_SCHEMA
FROM INFORMATION_SCHEMA.TABLES
WHERE TABLE_NAME like '%tbl_tyumon_end%'; -- AND TABLE_SCHEMA='dobar' ;

SELECT TABLE_NAME , TABLE_SCHEMA, COLUMN_NAME
FROM INFORMATION_SCHEMA.COLUMNS
WHERE COLUMN_NAME like '%select_pros%';  -- AND TABLE_NAME='mst_staticsource' 
